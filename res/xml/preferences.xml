<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" >
    <CheckBoxPreference
      android:key="checkbox"
      android:title="@string/pref_history"
      android:summary="@string/pref_history_summary"
      android:defaultValue="true"
    />
    
    <ListPreference
      android:key="list"
      android:title="@string/pref_bangprofile"
      android:summary="@string/pref_bangprofile_summary"
      android:entries="@array/pref_bang_list"
      android:entryValues="@array/pref_bang_list_value"
      android:dialogTitle="@string/pref_bangprofile" 
      android:defaultValue="0" />
    
    <CheckBoxPreference
      android:key="checkbox"
      android:title="@string/pref_browser"
      android:summary="@string/pref_browser_summary"
      android:defaultValue="true"
    />

</PreferenceScreen>